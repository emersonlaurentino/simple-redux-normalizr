{
  "name": "simple-redux-normalizr",
  "version": "0.0.2-0",
  "description": "Normalizes and denormalizes Arrays for Redux",
  "main": "dist/src/index.js",
  "bugs": {
    "url": "https://github.com/emersonlaurentino/simple-redux-normalizr/issues"
  },
  "homepage": "https://github.com/emersonlaurentino/simple-redux-normalizr",
  "repository": {
    "url": "https://github.com/emersonlaurentino/simple-redux-normalizr.git",
    "type": "git"
  },
  "scripts": {
    "build": "npm run clean && mkdirp dist && npm run build:node && npm-run-all --parallel build:development build:production",
    "build:development": "NODE_ENV=development rollup -c",
    "build:production": "NODE_ENV=production rollup -c",
    "build:node": "babel src/*.js -d dist",
    "clean": "rimraf dist",
    "lint": "eslint ./ --fix",
    "prebuild": "npm run clean",
    "prepublish": "npm run build"
  },
  "keywords": [
    "flux",
    "redux",
    "normalize",
    "denormalize",
    "api",
    "json"
  ],
  "author": "Emerson Laurentino",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.3.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
